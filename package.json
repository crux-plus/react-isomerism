    "flow-bin": "^0.57.3",
    "html-webpack-plugin": "^2.30.1",
    "koa": "^2.3.0",
    "koa-trie-router": "^2.1.6",
    "nodemon": "^1.12.1",
    "pm2": "^2.7.2",
    "react-dom": "^16.0.0",
    "react-router-component": "^0.39.0",
    "socket.io": "^2.0.4",
    "webpack-dev-server": "^2.9.3"
